  <%- include('partials/navbar') %>
  <h1><%= album.title %></h1>

  <div class = "row">
    <p>track #<p>
    <p>song title<p>
    <p>song length<p>
  </div>

    <% songs.forEach((song) => { %>
      <div class = "item row">
        <h3><%= song.track_no%><h3>
        <h2><%= song.title %></h2>
        <h3><%= Math.floor(song.length/60) %>:<%= song.length%60 %></h3>
      </div>
    <% }) %>
